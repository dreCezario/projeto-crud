<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head.ejs') %>
<body>
    
    <h1 class="title"> <%= title %> </h1>

    <%- include('partials/navbar.ejs') %>

    <ul class="users-list">
        <% users.forEach((user) => { %>
            <li class="user">
                <%= user.name %> - <%= user.email %> - Idade: <%= user.age %> - 
                <a href="/edit?id=<%= user._id %>">[editar]</a>
            </li>
        <% }) %>
    </ul>

    <%- include('partials/foot.ejs') %>
</body>
</html>